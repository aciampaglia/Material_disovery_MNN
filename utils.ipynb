    "def Re(theta):\n",
    "    t = np.deg2rad(theta)\n",
    "    c = np.cos(t)\n",
    "    s = np.sin(t)\n",
    "    R = [[c**2, s**2, c*s], [s**2, c**2, -c*s], [-2*c*s, 2*c*s, c**2 - s**2]]\n",
    "    return R"
   ]
  }
